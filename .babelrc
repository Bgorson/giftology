{
  "presets": ["@babel/env", "@babel/react"],
  "plugins": [
    ["ramda", { "useES": true }]
  ],
  "env": {
    "test": {
      "plugins": ["ramda"]
    }
  },
  "module":{
    "rules":[
      {
        "use": {
          "loader": "babel-loader",
          "options": {
            "failOnError":false,
            "failOnWarning":false,
            "presets": ["@babel/preset-env"]
          }
        }
      }
    ]
  }
  
}
